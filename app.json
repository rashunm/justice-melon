{
  "expo": {
    "name": "Load Testing App",
    "slug": "load-testing-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "description": "A mobile app for load testing websites with adjustable resource usage",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.loadtestingapp.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.loadtestingapp.app",
      "permissions": ["INTERNET", "WAKE_LOCK"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
